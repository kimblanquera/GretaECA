material default
{
	receive_shadows on
	technique
	{
		pass
		{
			ambient 0.4 0.3 0.25 1.0
			diffuse 0.64 0.48 0.4 1.0
			emissive 0.0 0.0 0.0 1.0
			cull_hardware none
			cull_software none
		}
	}
}
material eyelid
{
	receive_shadows on
	technique
	{
		pass
		{
			ambient 0.5 0.5 0.5 1.0
			diffuse 0.8 0.8 0.8 1.0
