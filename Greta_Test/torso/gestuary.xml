<?xml version="1.0"?>
<!DOCTYPE gestuary SYSTEM "gestuary.dtd" []>

<gestuary>

	<entry id="none">
		<verbatim>
		
		</verbatim>
		<form>
			<upperbody>
				<torso>
					
					<movement phase="stroke" trajectory="linear">
						<timing duration="0.3"/>
						<spine manner="opened"/>
						<point forward_backward="center" left_right="center" tilt="center" spine_rotation="center" compensate_head="0" compensate_shoulders="0" scaling="0.3"/>
					</movement>
				</torso>
			</upperbody>
		</form>
	</entry>

	<entry id="bow">
		<verbatim>
		this gesture is used to bow toward the interlocutor
		</verbatim>
		<form>
			<upperbody>
				<torso>
					<movement phase="stroke" trajectory="linear">
						<timing min="1" max="3" fixed="0"/>
						<spine manner="closed"/>
						<point forward_backward="forward" left_right="center" tilt="center" spine_rotation="center"
						compensate_head="0" compensate_shoulders="1" scaling="0.3"/>
					</movement>
				</torso>
			</upperbody>
		</form>
	</entry>
	
